  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(j|t)sx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "globals": {
      "VERSION": "1.0.0",
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  }
